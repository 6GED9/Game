<Window x:Class="Game.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Game"
        mc:Ignorable="d"
        Title="MainWindow" Height="920" Width="920" ResizeMode="NoResize" ContentRendered="Window_ContentRendered" KeyDown="Window_KeyDown" KeyUp="Window_KeyUp">
    <Grid Height="920" Width="920">
        <Canvas Name="GameArea" ClipToBounds="True" Width="750" Height="550" Grid.RowSpan="2">
            <Ellipse Name ="ellipse" Width="30" Height="30" Fill="LightPink" StrokeThickness="3" StrokeDashArray="2 2" Stroke="Gray" StrokeDashCap="Round" Canvas.Left="10" Canvas.Top="10" ></Ellipse>
            <Label Content="Label" Canvas.Left="-65" Canvas.Top="22"/>
        </Canvas>
        <Label Name="label" Content="Label" HorizontalAlignment="Left" Margin="94,54,0,0" VerticalAlignment="Top"/>
        <Label Name="extraText" Content="Жизни 0/1" HorizontalAlignment="Left" Margin="94,28,0,0" VerticalAlignment="Top"/>
        <Button Name="RestartButton" Content="Restart" HorizontalAlignment="Left" Margin="419,21,0,0" VerticalAlignment="Top" IsEnabled="False" Click="Button_Click"/>

    </Grid>
</Window>
